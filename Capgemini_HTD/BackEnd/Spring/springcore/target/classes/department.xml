<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- bean definitions here -->

	<bean id="dev" class="com.capgemini.springcore.beans.DepartmentBean" >
		<property name="deptId" value="901"></property>
		<property name="deptName" value="Dev"></property>
	</bean>
	
	<bean id="tes" class="com.capgemini.springcore.beans.DepartmentBean" primary="true">
		<property name="deptId" value="902"></property>
		<property name="deptName" value="tes"></property>
	</bean>
	
	
</beans>